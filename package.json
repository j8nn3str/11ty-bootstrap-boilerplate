{
  "name": "11ty-bootstrap-boilerplate",
  "version": "1.0.0",
  "description": "A quick-start boilerplate for 11ty with Bootstrap 5.3.",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel dev:*",
    "build": "run-s prod:_",
    "dev:eleventy": "eleventy --serve",
    "dev:parcel": "parcel watch ./src/_assets/js/main.js --out-dir ./_site/assets",
    "prod:eleventy": "eleventy",
    "prod:parcel": "parcel build ./src/_assets/js/main.js --out-dir ./_site/assets"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "sass": "^1.69.5"
  },
  "dependencies": {
    "bootstrap": "^5.3.2",
    "jquery": "^3.7.1",
    "parcel-bundler": "^1.12.5",
    "popper.js": "^1.16.1"
  }
}
